<!DOCTYPE html>
<html>
<% include ../../layouts/backend/header.ejs %>
<body>
    <form action="../update" method="post">
        <input type="text" hidden name="id" value="<%= user[0]['id'] %>">
        <div class="col-sm-12">
            <div class="form-group">
                <label>Name</label>
                <input type="text" name="txtName" class="form-control" value="<%= user[0]['name'] %>">
            </div>
            <div class="form-group">
                <label>Email</label>
                <input type="email" name="txtEmail" class="form-control" value="<%= user[0]['email'] %>" >
            </div>
            <button type="submit" class="btn btn-success btn-sm">Edit</button>
        </div>
    </form>
</body>
<script>
    $(document).ready(function(){
        
    });
</script>
</html>